    "bcrypt": "^5.0.0",
    "bull": "^3.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.8.1",
    "graphql": "^14.7.0",
    "graphql-middleware": "^6.0.4",
    "graphql-scalars": "^1.7.0",
    "graphql-shield": "^7.5.0",
    "ioredis": "^4.22.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.20",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.30.0",
    "socket.io": "^3.1.1",
    "tiny-lru": "^7.0.6",
    "uuid": "^8.3.2",
    "wordkit": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.15.0",
    "@types/express": "^4.17.9",
    "@types/got": "^9.6.11",
    "@types/ioredis": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-fetch": "^2.5.7",
    "@types/socket.io": "^2.1.12",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1",
    "prisma": "^2.16.1",
    "ts-node-dev": "1.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "yarn nexus:reflection:temp && tsc",
    "build:reflect": "cross-env NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true ts-node --transpile-only ./src/schema",
    "dev:migrate": "prisma migrate dev --preview-feature",
    "dev": "ts-node-dev --transpile-only ./src/server.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "format": "npx prettier --write './**/*.{ts,md}'",
    "start": "cross-env NODE_ENV=production node .nexus/build"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
